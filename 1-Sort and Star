// You will be given a vector of strings. You must sort it alphabetically (case-sensitive, and based on the ASCII values of the chars) and then return the first value.
// The returned value must be a string, and have "***" between each of its letters.
// You should not remove or add elements from/to the array.

function twoSort(s){
  let t = s.sort()
  let char = t[0]
  return char.split('').join('***')
}

input: twoSort(["bitcoin", "take", "over", "the", "world", "maybe", "who", "knows", "perhaps"])     output: b***i***t***c***o***i***n
input: twoSort(["turns", "out", "random", "test", "cases", "are", "easier", "than", "writing", "out", "basic", "ones"])    output: a***r***e
